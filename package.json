{
  "name": "justmd",
  "productName": "justmd",
  "version": "1.2.1",
  "description": "just markdown editor",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-x64": "electron-packager . --prune --overwrite --asar --out=dist --platform=win32 --arch=x64 --icon images/justmd.ico",
    "package-x86": "electron-packager . --prune --overwrite --asar --out=dist --platform=win32 --arch=ia32 --icon images/justmd.ico",
    "package-osx": "electron-packager . --prune --overwrite --asar --out=dist --platform=darwin --arch=x64 --icon images/justmd.ico",
    "package-l64": "electron-packager . --prune --overwrite --asar --out=dist --platform=linux --arch=x64  --icon images/justmd.png"
  },
  "devDependencies": {
    "electron": "^13.6.6",
    "electron-packager": "^15.2.0"
  }
}
